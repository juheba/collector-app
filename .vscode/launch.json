  "version": "0.2.0",
  "configurations": [
    {
      "name": "[Chrome] collector",
      "request": "launch",
      "type": "dart",
      "args": ["--web-port=3000", "--web-renderer=html"],
      "deviceId": "chrome",
    },
    {
      "name": "[Samsung] collector",
      "request": "launch",
      "type": "dart",
      "deviceId": "SM G973F",
    },
    {
      "name": "collector (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "collector (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}